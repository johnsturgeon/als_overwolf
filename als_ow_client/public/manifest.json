{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Apex Legends Status - Extra Stats",
    "author": "John Sturgeon",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.160.0",
    "icon": "icons/icon.png",
    "icon_gray": "icons/icon_gray.png",
    "description": "Game Events Consumer Sample"
  },
  "__TODO__": "JHS: Do I really need 'permissions'",
  "permissions": [
    "GameInfo"
  ],
  "data": {
    "start_window": "background",
    "externally_connectable": {
      "matches": [
        "http://192.168.1.175:8822",
        "https://apexlegendsstatus.com"
      ]
    },
    "force_browser": "user",
    "windows": {
      "background": {
        "file": "background.html",
        "background_optimization": false,
        "is_background_page": true
      },
      "desktop": {
        "file": "desktop.html",
        "desktop_only": true,
        "native_window": true,
        "resizable": true,
        "transparent": true,
        "override_on_update": true,
        "size": {
          "width": 1212,
          "height": 699
        },
        "min_size": {
          "width": 1212,
          "height": 699
        }
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        21566
      ]
    },
    "game_events": [21566],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            21566
          ]
        },
        "start_minimized": false
      }],
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}